<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>WhoAndAboutWhoFieldForSurveys</fullName>
    <active>true</active>
    <errorConditionFormula>AND(CONTAINS(RecordType.DeveloperName, &apos;Survey&apos;),  OR(ISBLANK(Who__c), ISBLANK(AboutWho__c)))</errorConditionFormula>
    <errorMessage>Please fill the Who and About Who fields</errorMessage>
</ValidationRule>
