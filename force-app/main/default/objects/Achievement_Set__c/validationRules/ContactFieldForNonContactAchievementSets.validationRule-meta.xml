<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ContactFieldForNonContactAchievementSets</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(CONTAINS(RecordType.DeveloperName, &apos;Contact&apos;)),  NOT(ISBLANK(Contact__c)))</errorConditionFormula>
    <errorMessage>Field Contact cannot be filled for non Contact Achievement Sets</errorMessage>
</ValidationRule>
